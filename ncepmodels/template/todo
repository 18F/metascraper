  <gmd:maximumValue>
   <gco:Real> 3100 </gco:Real>
  </gmd:maximumValue>
  <gmd:verticalCRS xlink:href="http://www.epsg-registry.org/export.htm?gml=urn:ogc:def:crs:EPSG::5715" xlink:title="msl depth in meters" xlink:actuate="onRequest"/>
 </gmd:EX_VerticalExtent>
</gmd:verticalElement>

